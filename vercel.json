{
    "version": 2,
    "routes": [
        {
            "src": "/animals/(.*)",
            "dest": "views/animals",
            "headers": {
                "Access-Control-Allow-Origin": "http://localhost:3000"
            }
        },
        {
            "src": "/animal/(.*)",
            "dest": "views/animal",
            "headers": {
                "Access-Control-Allow-Origin": "http://localhost:3000"
            }
        },
        {
            "src": "/(.*)",
            "dest": "index.js",
            "headers": {
                "Access-Control-Allow-Origin": "http://localhost:3000"
            }
        }
    ]
}